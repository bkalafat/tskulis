  "name": "tskulis",
  "version": "1.0.0",
  "description": "tskulis",
  "private": true,
  "main": "src/index.js",
  "dependencies": {
    "@ckeditor/ckeditor5-build-classic": "^34.2.0",
    "@ckeditor/ckeditor5-editor-classic": "^34.2.0",
    "@ckeditor/ckeditor5-image": "^34.2.0",
    "@ckeditor/ckeditor5-react": "^5.0.3",
    "@next-auth/mongodb-adapter": "^1.0.4",
    "@popperjs/core": "^2.11.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.3.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "axios": "^0.27.2",
    "bootstrap": "^4.6.2",
    "cli-progress": "^3.12.0",
    "cross-env": "^7.0.3",
    "isomorphic-unfetch": "^3.1.0",
    "jquery": "^3.6.1",
    "lighthouse": "^12.8.2",
    "mongodb": "^4.12.1",
    "next": "^12.3.4",
    "next-auth": "^4.18.0",
    "next-sitemap": "^3.1.32",
    "npm": "^8.15.0",
    "popper.js": "^1.16.1",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.6.0",
    "react-bootstrap-table-next": "^4.0.3",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-image-file-resizer": "^0.4.8",
    "react-scripts": "^5.0.1",
    "react-share": "^4.4.1",
    "react-slick": "^0.29.0",
    "react-social-icons": "^5.15.0",
    "reactjs-popup": "^2.0.5",
    "reflect-metadata": "^0.1.13",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.6.5",
    "styled-components": "^5.3.11",
    "swr": "^1.3.0",
    "typescript-coverage-report": "^0.6.4",
    "watermarkjs": "^2.1.1",
    "web-vitals": "^5.1.0",
    "workbox-webpack-plugin": "^6.5.4"
  },
  "scripts": {
    "debug": "cross-env NODE_OPTIONS='--inspect' next dev",
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "cross-env ANALYZE=true next build",
    "build:production": "cross-env NODE_ENV=production next build && npm run build:optimize",
    "build:optimize": "npm run build:analyze && npm run postbuild",
    "start": "next start",
    "export": "next export -o build/",
    "postbuild": "next-sitemap --config next-sitemap.config.js && npm run sitemap:generate",
    "sitemap:generate": "node scripts/generate-sitemap.js",
    "sitemap:validate": "node scripts/validate-sitemap.js",
    "test": "jest --watch",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "ts-coverage": "typescript-coverage-report",
    "performance:audit": "npm run build && npm start & sleep 10 && node scripts/lighthouse-audit.js && npm run performance:kill",
    "performance:kill": "taskkill /F /IM node.exe /FI \"WINDOWTITLE eq *next*\" || true",
    "lighthouse": "node scripts/lighthouse-audit.js",
    "lighthouse:dev": "node scripts/lighthouse-audit.js",
    "performance:report": "node scripts/performance-report.js",
    "performance:full": "npm run lighthouse && npm run performance:report",
    "deploy:staging": "bash scripts/deploy/deploy.sh -e staging",
    "deploy:production": "bash scripts/deploy/deploy.sh -e production",
    "deploy:quick": "bash scripts/deploy/deploy.sh -s -b",
    "rollback:staging": "bash scripts/deploy/rollback.sh -e staging",
    "rollback:production": "bash scripts/deploy/rollback.sh -e production",
    "rollback:list": "bash scripts/deploy/rollback.sh -l",
    "setup:security": "bash scripts/setup-security-monitoring.sh",
    "validate:env": "node scripts/validate-environment.js",
    "backup:db": "bash scripts/backup/backup-database.sh",
    "backup:app": "bash scripts/backup/backup-app.sh",
    "size:check": "npm run build:analyze && echo 'Check ./analyze/ folder for bundle analysis'",
    "clean": "rm -rf .next build analyze coverage dist",
    "precommit": "npm run test:ci && npm run build",
    "docs:api": "typedoc",
    "docs:components": "node scripts/generate-component-docs.js",
    "docs:generate": "npm run docs:api && npm run docs:components",
    "docs:serve": "npx http-server docs -p 3001 -o",
    "docs:build": "npm run docs:generate && npm run docs:optimize",
    "docs:optimize": "node scripts/optimize-docs.js",
    "docs:validate": "node scripts/validate-docs.js",
    "docs:update": "node scripts/update-docs.js",
    "docs:clean": "rm -rf docs/api docs/components",
    "docs:dev": "npm run docs:generate && npm run docs:serve"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@microsoft/tsdoc": "^0.15.1",
    "@next/bundle-analyzer": "^15.5.4",
    "@playwright/test": "^1.55.1",
    "@swc/core": "^1.13.5",
    "@swc/jest": "^0.2.39",
    "@types/node": "^18.11.10",
    "@types/react-bootstrap-table-next": "^4.0.18",
    "@types/react-slick": "^0.23.10",
    "@types/styled-components": "^5.1.34",
    "@types/supertest": "^6.0.3",
    "babel-jest": "^28.1.3",
    "critters": "^0.0.23",
    "eslint-config-next": "^12.3.4",
    "jest": "^28.1.3",
    "jsdoc": "^4.0.4",
    "node-mocks-http": "^1.17.2",
    "sass": "^1.93.2",
    "supertest": "^7.1.4",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typedoc-plugin-mermaid": "^1.12.0",
    "typescript": "^4.9.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
